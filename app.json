{
  "expo": {
    "name": "cuidador-app",
    "slug": "cuidador-app",
    "scheme": "cuidador-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.takeeko.cuidadorapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.applesignin": [
          "Default",
          "Default"
        ]
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.takeeko.cuidadorapp",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "d4cf8bc9-65fd-4f49-a415-c9b96ec68391"
      }
    },
    "owner": "takeeko",
    "plugins": [
      "expo-web-browser",
      "expo-font"
    ],
    "policy": "appVersion",
    "updates": {
      "url": "https://u.expo.dev/d4cf8bc9-65fd-4f49-a415-c9b96ec68391",
      "enabled": true,
      "fallbackToCacheTimeout": 0
    }
  }
}
